  
<Dashboard>:
    MDFloatLayout:
        md_bg_color:1,1,1,1
        MDIconButton:
            icon:"arrow-left"
            pos_hint:{"center_y": .95}
            user_font_size:"30sp"
            theme_text_color:"Custom"
            text_color: rgba(26,24,58,255)
            on_release:
                root.manager.transition.direction="right"
                root.manager.current="homescreen"
        MDLabel:
            text:"Welcome to the dashboard"
            #font_name:"BPoppins"
            font_size:"26sp"
            pos_hint:{"center_x": .5, "center_y": .85}
            
            color:rgba(0,0,54,255)
    
    MDBoxLayout:
        size_hint_y: None
        md_bg_color: 0, 0, 0, 0
        NavBar:
            one: [1, 1, 1, 1]